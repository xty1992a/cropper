{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "useBuiltIns": "usage",
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions"
          ]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    [
      "transform-react-jsx",
      {
        "pragma": "preact.h"
      }
    ]
  ]
}
